@PACKAGE_INIT@

cmake_minimum_required(VERSION 3.17)

@Morpheus_BUILD_DIR_EXPORT_SETTINGS@

# make the bundled find modules in this directory available
list(PREPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

include(CMakeFindDependencyMacro)

include("${CMAKE_CURRENT_LIST_DIR}/MorpheusTargets.cmake")
include("${CMAKE_CURRENT_LIST_DIR}/MorpheusConfigVersion.cmake")

if(@Morpheus_CUDA@)
    find_dependency(CUDAToolkit)
endif()

if(@Morpheus_THRUST@)
    # Invokes FindThrust.cmake
    find_dependency(Thrust @Morpheus_MIN_VERSION_Thrust@ REQUIRED CONFIG)
    thrust_create_target(morpheus::Thrust FROM_OPTIONS)
endif()

list(POP_FRONT CMAKE_MODULE_PATH)

check_required_components(morpheus)

set(${CMAKE_FIND_PACKAGE_NAME}_CONFIG "${CMAKE_CURRENT_LIST_FILE}")

include(FindPackageHandleStandardArgs)
find_package_handle_standard_args(${CMAKE_FIND_PACKAGE_NAME} CONFIG_MODE)