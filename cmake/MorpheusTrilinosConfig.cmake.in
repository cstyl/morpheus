IF (NOT TARGET Morpheus::morpheus)
  # Compute the installation prefix relative to this file.
  get_filename_component(MORPHEUS_IMPORT_PREFIX "${CMAKE_CURRENT_LIST_FILE}" PATH)
  get_filename_component(MORPHEUS_IMPORT_PREFIX "${MORPHEUS_IMPORT_PREFIX}" PATH)
  get_filename_component(MORPHEUS_IMPORT_PREFIX "${MORPHEUS_IMPORT_PREFIX}" PATH)
  get_filename_component(MORPHEUS_IMPORT_PREFIX "${MORPHEUS_IMPORT_PREFIX}" PATH)
  if(MORPHEUS_IMPORT_PREFIX STREQUAL "/")
    set(MORPHEUS_IMPORT_PREFIX "")
  endif()
  add_library(Morpheus::morpheus INTERFACE IMPORTED)
  set_target_properties(Morpheus::morpheus PROPERTIES
    INTERFACE_LINK_LIBRARIES "@Morpheus_LIBRARIES@;@MORPHEUS_LINK_OPTIONS@"
    INTERFACE_COMPILE_FEATURES "@MORPHEUS_CXX_STANDARD_FEATURE@"
    INTERFACE_COMPILE_OPTIONS "@MORPHEUS_ALL_COMPILE_OPTIONS@"
    INTERFACE_INCLUDE_DIRECTORIES "${MORPHEUS_IMPORT_PREFIX}/include"
  )
ENDIF()