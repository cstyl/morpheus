// MORPHEUS_VERSION % 100 is the patch level
// MORPHEUS_VERSION / 100 % 100 is the minor version
// MORPHEUS_VERSION / 10000 is the major version
#cmakedefine MORPHEUS_VERSION @MORPHEUS_VERSION@

/* Execution Spaces */
#cmakedefine MORPHEUS_ENABLE_SERIAL
#cmakedefine MORPHEUS_ENABLE_OPENMP
#cmakedefine MORPHEUS_ENABLE_CUDA

/* General Settings */
#cmakedefine MORPHEUS_ENABLE_CXX17
#cmakedefine MORPHEUS_ENABLE_CXX20

#cmakedefine MORPHEUS_ENABLE_CUDA_RELOCATABLE_DEVICE_CODE
#cmakedefine MORPHEUS_ENABLE_CUDA_LAMBDA
#cmakedefine MORPHEUS_ENABLE_CUDA_CONSTEXPR
#cmakedefine MORPHEUS_ENABLE_DEBUG
#cmakedefine MORPHEUS_ENABLE_COMPILER_WARNINGS
#cmakedefine MORPHEUS_ENABLE_DEPRECATED_CODE

